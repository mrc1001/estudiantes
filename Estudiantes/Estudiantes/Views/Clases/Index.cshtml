@model IEnumerable<Estudiantes.Models.ORM.Clase>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreClase)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreClase)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IdClase }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.IdClase })
        </td>
    </tr>
}

</table>
